<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
<mapper namespace="com.yedam.mapper.MemberMapper">
  <select id="selectMember" resultType="member">
    SELECT *
    FROM  tbl_member
    WHERE user_id  = #{id}
      AND password = #{pw}
  </select>
</mapper>